fun main(){
    var characterArray=arrayOf('i','n','t','e','r')

    val charArray= arrayOf('a','b','c','d','e')

    val otherArray=arrayOf('l','m','n','o','p')

    //characterArray=otherArray **this is not possible**

    val newArray=arrayOf("Hello",12,34.5,true,'x')

}

//val_type_array we can change the value of individual elements.
//var_type_array we can do both alteration and assignment to new array.